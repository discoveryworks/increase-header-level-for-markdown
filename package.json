{
	"name": "markdown-lower-headers-levels",
	"displayName": "Markdown Lower Headers Levels",
	"description": "A tool for making levels of headers lower in Markdown",
	"version": "1.0.0",
	"publisher": "ruichima",
	"repository": {
		"type": "git",
		"url":"https://github.com/ruichima/markdown-lower-headers-levels"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.markdown_lower_headers_levels"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.markdown_lower_headers_levels",
				"title": "Markdown Lower Headers Levels"
			}
		],
		"keybindings": [
			{
				"command": "extension.markdown_lower_headers_levels",
				"key": "ctrl+shift+3"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
