{
	"name": "increase-header-level-for-markdown",
	"displayName": "Increase Header Level for Markdown",
	"description": "Increase header hierarchy (e.g., h2=>h1) in Markdown",
	"version": "1.2.0",
	"publisher": "discoveryworks",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonathanpberger/increase-header-level-for-markdown"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.increase_header_level_for_markdown"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.increase_header_level_for_markdown",
				"title": "increase header level for Markdown"
			}
		],
		"keybindings": [
			{
				"command": "extension.increase_header_level_for_markdown",
				"key": "ctrl+shift+4"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
